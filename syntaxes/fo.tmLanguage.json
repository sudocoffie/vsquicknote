{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "fo",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.fo",
          "match": "\\b(if|while|for|return)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.fo",
      "begin": "âœ”",
      "end": "||",
      "patterns": [
        {
          "name": "constant.character.escape.fo",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.fo"
}
